<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:contentInsetLeft="0dp"
    android:contentInsetStart="0dp"
    app:contentInsetLeft="0dp"
    app:contentInsetStart="0dp"
    android:contentInsetRight="0dp"
    android:contentInsetEnd="0dp"
    app:contentInsetRight="0dp"
    app:contentInsetEnd="0dp"
    android:background="#FFFFFF"
    android:minHeight="76dp"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        >

        <ImageView
            android:id="@+id/logobt"
            android:layout_width="100dp"
            android:layout_height="43dp"
            android:layout_centerInParent="true"
            android:layout_marginTop="6dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:src="@drawable/logo2" />
        <LinearLayout
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/menubt"
            android:layout_marginLeft="12dp"
            android:orientation="vertical"
            android:layout_centerVertical="true"
            android:focusable="false"
            android:focusableInTouchMode="false"
            >
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/menubt_3"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:layout_marginTop="5dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:orientation="horizontal"
            android:layout_marginRight="19dp"
            >

            <ImageView
                android:id="@+id/bt20"
                android:layout_width="27dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:src="@drawable/contsbt" />
            <ImageView
                android:id="@+id/schbt"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical"
                android:focusable="false"
                android:layout_marginLeft="10dp"
                android:focusableInTouchMode="false"
                android:src="@drawable/schbt" />
            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:id="@+id/getsendstr_bt"
                android:src="@drawable/conts2bt"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                />
            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:id="@+id/user_imgsrc"
                android:src="@drawable/user_icon"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="6dp"
                />
        </LinearLayout>

    </RelativeLayout>
</androidx.appcompat.widget.Toolbar>
